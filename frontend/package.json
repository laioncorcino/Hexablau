{
  "name": "hexablau",
  "version": "1.0.0",
  "description": "Hexablau",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack:dev",
    "webpack:dev": "webpack-dev-server --config webpack.config.dev.js",
    "webpack:prod": "webpack --config webpack.config.prod.js",
    "share": "ngrok http --host-header=rewrite 3099",
    "build": "npm-run-all --serial clean-dist webpack:prod",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint \\\"src/scripts/!(vendor)/**/*.js\\\"",
    "lint:scss": "stylelint \\\"src/styles/*.scss\\\"",
    "remove-dist": "rimraf \"dist\"",
    "clean-dist": "npm run remove-dist && mkdir \"dist\""
  },
  "settings": {
    "envs": {
      "development": "http://localhost:4000/hexablau/api/"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Projeto-de-Software-2021-1/Hexablau"
  },
  "author": "Elihofni Lima",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^1.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "ngrok": "^3.2.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.4.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "sass-mq": "^5.0.1",
    "style-loader": "^0.23.1",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "3.6.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.6.2",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "dropzone": "^5.5.1",
    "happypack": "^5.0.1",
    "jquery": "^3.4.1",
